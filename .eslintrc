{
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": "warn",
    "no-unsafe-finally": "off"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ]
}
